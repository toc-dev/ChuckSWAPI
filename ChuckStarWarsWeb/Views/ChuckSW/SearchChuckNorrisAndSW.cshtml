@model ChuckSWShared.Dtos.SearchResultsDto

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<table class="table">
		<thead>
			<tr>
				<th>
					Name
				</th>
				<th>
					Search Type
				</th>
			</tr>
		</thead>

		<tbody>
			<@foreach (var searchResult in Model.Results) { 
				<tr>
					<div class="modal fade" id="jokeCategory" aria-labelledby="getRandomJokesModalLabel" aria-hidden="true">
						<td> @searchResult.Value </td>
						<td> @searchResult.SearchType </td>
					</div>
				
				</tr>
				}
		</tbody>

</table>


   